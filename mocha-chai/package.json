{
  "name": "Mocha-Chai",
  "version": "1.0.0",
  "description": "Unit test basic with mocha and chai",
  "main": "app.js",
  "author": "Remy Nguyá»…n",
  "license": "MIT",
  "scripts": {
    "start": "nodemon --exec babel-node \"src/app.js\"",
    "build": "babel src -d lib",
    "test": "babel-node node_modules/mocha/bin/_mocha \"src/test/**/*.js\"",
    "test:watch": "babel-node node_modules/mocha/bin/_mocha --watch \"src/test/**/*.js\"",
    "coverage": "rm -rf coverage && babel-node node_modules/.bin/babel-istanbul cover node_modules/mocha/bin/_mocha \"src/test/**/*.js\" || true",
    "coverage-win": "babel-istanbul cover -x 'types/unused/**' -x 'types/enums/**' node_modules/mocha/bin/_mocha \"src/test/**/*.js\""
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-istanbul": "^0.12.2",
    "babel-plugin-root-import": "^5.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "nodemon": "^1.17.3"
  },
  "dependencies": {
    "chai": "^4.1.2",
    "mocha": "^5.0.4",
    "sinon": "^5.0.3"
  }
}
